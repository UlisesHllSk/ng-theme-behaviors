{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Angular/Karma Test Explorer Debugger",
      "port": 9222,
      "webRoot": "${workspaceRoot}",
      "pathMapping": {
        "/": "${workspaceRoot}",
        "/base/": "${workspaceRoot}/"
      },
      "sourceMapPathOverrides": {
        "*": "${webRoot}/*"
      },
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Karma Chrome Debugger",
      "address": "localhost",
      "webRoot": "${workspaceRoot}",
      "port": 9333,
      "pathMapping": {
        "/": "${workspaceRoot}",
        "/base/": "${workspaceRoot}/"
      },
      "sourceMapPathOverrides": {
        "*": "${webRoot}/*"
      },
    }
  ]
}